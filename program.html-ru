<script type="text/javascript">
//<![CDATA[
	function fn_follow_user(uid,rid)
	{
	if ($('#'+rid).hasClass('checked')){

		fn_follow_user_delete(uid,rid);
	}else{
		$.ajax({
			type: "POST",
			url: "index.php?dispatch=follow.program",
			data: "&add_report=1&uid="+uid+"&rid="+rid,
			success: function(data) {
				fn_follow_change_report(data.substring(0,data.indexOf('|')));
			}
		});
		$('#report_'+rid).addClass('hidden');

		return false;
		};
	}
	function fn_follow_change_report(id)
	{
		$('#r-'+id).addClass('checked');
		$('.sec-'+id).addClass('is-checked');
		$('.sec-'+id).removeClass('darker');
		return false;
	}
//]]>
</script>




<script type="text/javascript">
//<![CDATA[
	function fn_follow_user_delete(uid,rid)
	{
		$.ajax({
			type: "POST",
			url: "index.php?dispatch=follow.program",
			data: "&delete_report=1&uid="+uid+"&rid="+rid,
			success: function(data) {
				var id = data.substring(0,data.indexOf('|'));
				$('div#r-'+id).empty();
				$('.sec-'+id).removeClass('is-checked');
				$('#r-'+id).removeClass('checked');
			}
		});
		return false;
	}
//]]>
</script>



